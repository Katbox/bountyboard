<div id="bountyPostArea" class="span11 offset2">
	<%= form_for @bounty do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %>
        <%= f.label :desc %>
        <%= f.text_area :desc %>
        <%= f.label :price %>
        <%= f.text_field :price %>
        <%= f.submit "Post Your Bounty!" %>
    <% end %>
</div>
